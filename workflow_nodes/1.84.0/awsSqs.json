{
  "nodeType": "n8n-nodes-base.awsSqs",
  "displayName": "AWS SQS",
  "description": "Sends messages to AWS SQS",
  "version": 1,
  "properties": [
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "sendMessage",
      "description": "Send a message to a queue",
      "options": [
        {
          "name": "Send Message",
          "value": "sendMessage",
          "description": "Send a message to a queue"
        }
      ]
    },
    {
      "name": "queue",
      "displayName": "Queue Name or ID",
      "type": "options",
      "default": "",
      "description": "Queue to send a message to. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "sendMessage"
          ]
        }
      }
    },
    {
      "name": "queueType",
      "displayName": "Queue Type",
      "type": "options",
      "default": "standard",
      "description": "FIFO SQS queue",
      "options": [
        {
          "name": "FIFO",
          "value": "fifo",
          "description": "FIFO SQS queue"
        },
        {
          "name": "Standard",
          "value": "standard",
          "description": "Standard SQS queue"
        }
      ]
    },
    {
      "name": "sendInputData",
      "displayName": "Send Input Data",
      "type": "boolean",
      "default": true,
      "description": "Whether to send the data the node receives as JSON to SQS"
    },
    {
      "name": "message",
      "displayName": "Message",
      "type": "string",
      "default": "",
      "description": "Message to send to the queue",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "sendMessage"
          ],
          "sendInputData": [
            false
          ]
        }
      }
    },
    {
      "name": "messageGroupId",
      "displayName": "Message Group ID",
      "type": "string",
      "default": "",
      "description": "Tag that specifies that a message belongs to a specific message group. Applies only to FIFO (first-in-first-out) queues.",
      "required": true,
      "displayOptions": {
        "show": {
          "queueType": [
            "fifo"
          ]
        }
      }
    },
    {
      "name": "options",
      "displayName": "Options",
      "type": "collection",
      "default": {},
      "description": "How long, in seconds, to delay a message for",
      "placeholder": "Add option",
      "typeOptions": {
        "minValue": 0,
        "maxValue": 900
      },
      "displayOptions": {
        "show": {
          "operation": [
            "sendMessage"
          ]
        }
      }
    }
  ],
  "credentialsConfig": [
    {
      "name": "aws",
      "required": true
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "queue",
      "required": true
    },
    {
      "name": "sendInputData",
      "required": true
    },
    {
      "name": "number",
      "required": false
    },
    {
      "name": "string",
      "required": false
    },
    {
      "name": "messageDeduplicationId",
      "required": false
    }
  ],
  "io": {
    "inputs": [],
    "outputs": [],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [],
    "produces": []
  }
}