      "name": "Delete",
      "default": null,
      "description": "Delete a contact"
    },
    {
      "name": "Get",
      "default": null,
      "description": "Get a contact"
    },
    {
      "name": "Get Many",
      "default": null,
      "description": "Get many contacts"
    },
    {
      "name": "triggerId",
      "displayName": "Trigger Name or ID",
      "type": "options",
      "default": "",
