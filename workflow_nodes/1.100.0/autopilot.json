      "placeholder": "Add Custom Field"
    },
    {
      "name": "value",
      "displayName": "Value",
      "type": "string",
      "default": "",
      "description": "User-specified value of user-defined data"
    },
    {
      "name": "Fax",
      "displayName": "Fax",
      "type": "string",
      "default": ""
    },
    {
      "name": "FirstName",
      "displayName": "First Name",
      "type": "string",
      "default": ""
