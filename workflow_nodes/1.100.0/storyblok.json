{
  "nodeType": "n8n-nodes-base.storyblok",
  "displayName": "Storyblok",
  "description": "Consume Storyblok API",
  "version": 1,
  "properties": [
    {
      "name": "identifier",
      "displayName": "Identifier",
      "type": "string",
      "default": "",
      "description": "The ID or slug of the story to get",
      "required": true
    },
    {
      "name": "limit",
      "displayName": "Limit",
      "type": "number",
      "default": 50,
      "description": "Max number of results to return"
    },
    {
      "name": "filters",
      "displayName": "Filters",
      "type": "collection",
      "default": {},
      "description": "Filter by slug",
      "placeholder": "Add Filter"
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": null,
      "description": "Get a story"
    },
    {
      "name": "Get Many",
      "default": null,
      "description": "Get many stories"
    },
    {
      "name": "space",
      "displayName": "Space ID",
      "type": "options",
      "default": "",
      "description": "The name of the space.",
      "placeholder": "Add Field",
      "required": true,
      "options": []
    },
    {
      "name": "storyId",
      "displayName": "Story ID",
      "type": "string",
      "default": "",
      "description": "Numeric ID of the story",
      "required": true
    },
    {
      "name": "returnAll",
      "displayName": "Return All",
      "type": "boolean",
      "default": false,
      "description": "Whether to return all results or only up to a given limit"
    },
    {
      "name": "options",
      "displayName": "Options",
      "type": "collection",
      "default": {},
      "description": "Numeric ID of release",
      "placeholder": "Add option"
    },
    {
      "name": "language",
      "displayName": "Language",
      "type": "string",
      "default": "",
      "description": "Language code to publish the story individually (must be enabled in the space settings)"
    },
    {
      "name": "Delete",
      "default": null,
      "description": "Delete a story"
    },
    {
      "name": "Get",
      "default": null,
      "description": "Get a story"
    },
    {
      "name": "Publish",
      "default": null,
      "description": "Publish a story"
    },
    {
      "name": "Unpublish",
      "default": null,
      "description": "Unpublish a story"
    },
    {
      "name": "source",
      "displayName": "Source",
      "type": "options",
      "default": "contentApi",
      "description": "Pick where your data comes from, Content or Management API"
    },
    {
      "name": "Management API",
      "default": null
    },
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": null
    }
  ],
  "credentialsConfig": [
    {
      "name": "storyblokContentApi",
      "required": true
    },
    {
      "name": "source",
      "required": false
    },
    {
      "name": "resource",
      "required": false
    },
    {
      "name": "resource",
      "required": false
    }
  ]
}