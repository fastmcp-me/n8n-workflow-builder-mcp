      "required": true
    }
  ],
  "credentialsConfig": [
    {
      "name": "googleApi",
      "required": true
    },
    {
      "name": "authentication",
      "required": false
    },
    {
      "name": "authentication",
      "required": false
    },
    {
      "name": "resource",
      "required": false
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "text",
      "required": true
    }
  ]
}