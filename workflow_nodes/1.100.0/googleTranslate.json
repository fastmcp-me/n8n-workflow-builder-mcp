{
  "nodeType": "n8n-nodes-base.googleTranslate",
  "displayName": "Google Translate",
  "description": "Translate data using Google Translate",
  "version": [
    1,
    2
  ],
  "properties": [
    {
      "name": "authentication",
      "displayName": "Authentication",
      "type": "options",
      "default": null
    },
    {
      "name": "OAuth2",
      "default": null
    },
    {
      "name": "Service Account",
      "default": null
    },
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": null
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": null,
      "description": "Translate data"
    },
    {
      "name": "text",
      "displayName": "Text",
      "type": "string",
      "default": "",
      "description": "The input text to translate",
      "required": true
    },
    {
      "name": "translateTo",
      "displayName": "Translate To",
      "type": "options",
      "default": "",
