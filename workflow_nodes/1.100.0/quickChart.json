{
  "nodeType": "n8n-nodes-base.quickChart",
  "displayName": "QuickChart",
  "description": "Create a chart via QuickChart",
  "version": 1,
  "properties": [
    {
      "name": "chartType",
      "displayName": "Chart Type",
      "type": "options",
      "default": "bar",
      "description": "The type of chart to create"
    },
    {
      "name": "labelsMode",
      "displayName": "Add Labels",
      "type": "options",
      "default": null
    },
    {
      "name": "From Array",
      "default": null
    },
    {
      "name": "labelsUi",
      "displayName": "Labels",
      "type": "fixedCollection",
      "default": {},
      "description": "Labels to use in the chart",
      "placeholder": "Add Label",
      "required": true
    },
    {
      "name": "labelsArray",
      "displayName": "Labels Array",
      "type": "string",
      "default": "",
      "description": "The array of labels to be used in the chart",
      "placeholder": "e.g. [\"Berlin\", \"Paris\", \"Rome\", \"New York\"]",
      "required": true
    },
    {
      "name": "data",
      "displayName": "Data",
      "type": "json",
      "default": "",
      "description": "Data to use for the dataset, documentation and examples <a href=\"https://quickchart.io/documentation/chart-types/\" target=\"_blank\">here</a>",
      "placeholder": "e.g. [60, 10, 12, 20]",
      "required": true
    },
    {
      "name": "output",
      "displayName": "Put Output In Field",
      "type": "string",
      "default": "data",
      "description": "The binary data will be displayed in the Output panel on the right, under the Binary tab",
      "required": true
    },
    {
      "name": "chartOptions",
      "displayName": "Chart Options",
      "type": "collection",
      "default": {},
      "description": "Background color of the chart",
      "placeholder": "Add option"
    },
    {
      "name": "devicePixelRatio",
      "displayName": "Device Pixel Ratio",
      "type": "number",
      "default": 2,
      "description": "Pixel ratio of the chart"
    },
    {
      "name": "format",
      "displayName": "Format",
      "type": "options",
      "default": "png",
      "description": "File format of the resulting chart"
    },
    {
      "name": "PDF",
      "default": null
    },
    {
      "name": "SVG",
      "default": null
    },
    {
      "name": "WebP",
      "default": null
    },
    {
      "name": "height",
      "displayName": "Height",
      "type": "number",
      "default": 300,
      "description": "Height of the chart"
    },
    {
      "name": "horizontal",
      "displayName": "Horizontal",
      "type": "boolean",
      "default": false,
      "description": "Whether the chart should use its Y axis horizontal"
    },
    {
      "name": "width",
      "displayName": "Width",
      "type": "number",
      "default": 500,
      "description": "Width of the chart"
    },
    {
      "name": "datasetOptions",
      "displayName": "Dataset Options",
      "type": "collection",
      "default": {},
      "description": "Color used for the background the dataset (area of a line graph, fill of a bar chart, etc.)",
      "placeholder": "Add option"
    },
    {
      "name": "borderColor",
      "displayName": "Border Color",
      "type": "color",
      "default": "",
      "description": "Color used for lines of the dataset"
    },
    {
      "name": "fill",
      "displayName": "Fill",
      "type": "boolean",
      "default": true,
      "description": "Whether to fill area of the dataset"
    },
    {
      "name": "label",
      "displayName": "Label",
      "type": "string",
      "default": "",
      "description": "The label of the dataset"
    },
    {
      "name": "pointStyle",
      "displayName": "Point Style",
      "type": "options",
      "default": "circle",
      "description": "Style to use for points of the dataset"
    },
    {
      "name": "Cross",
      "default": null
    },
    {
      "name": "CrossRot",
      "default": null
    },
    {
      "name": "Dash",
      "default": null
    },
    {
      "name": "Line",
      "default": null
    },
    {
      "name": "Rect",
      "default": null
    },
    {
      "name": "Rect Rot",
      "default": null
    },
    {
      "name": "Rect Rounded",
      "default": null
    },
    {
      "name": "Star",
      "default": null
    },
    {
      "name": "Triangle",
      "default": null
    }
  ],
  "credentialsConfig": []
}