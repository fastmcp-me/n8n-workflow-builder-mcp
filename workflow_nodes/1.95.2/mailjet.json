{
  "nodeType": "n8n-nodes-base.mailjet",
  "displayName": "Mailjet",
  "description": "Consume Mailjet API",
  "version": 1,
  "properties": [
    {
      "name": "fromEmail",
      "displayName": "From Email",
      "type": "string",
      "default": "",
      "description": "The title for the email",
      "placeholder": "admin@example.com",
      "required": true
    },
    {
      "name": "toEmail",
      "displayName": "To Email",
      "type": "string",
      "default": "",
      "description": "Email address of the recipient. Multiple ones can be separated by comma.",
      "placeholder": "info@example.com",
      "required": true
    },
    {
      "name": "subject",
      "displayName": "Subject",
      "type": "string",
      "default": "",
      "description": "Subject line of the email",
      "placeholder": "My subject line"
    },
    {
      "name": "text",
      "displayName": "Text",
      "type": "string",
      "default": "",
      "description": "Plain text message of email"
    },
    {
      "name": "html",
      "displayName": "HTML",
      "type": "string",
      "default": "",
      "description": "HTML text message of email"
    },
    {
      "name": "jsonParameters",
      "displayName": "JSON Parameters",
      "type": "boolean",
      "default": false
    },
    {
      "name": "additionalFields",
      "displayName": "Additional Fields",
      "type": "collection",
      "default": {},
      "description": "Bcc Email address of the recipient. Multiple ones can be separated by comma.",
      "placeholder": "Add Field"
    },
    {
      "name": "ccAddresses",
      "displayName": "Cc Addresses",
      "type": "string",
      "default": "",
      "description": "Cc Email address of the recipient. Multiple ones can be separated by comma."
    },
    {
      "name": "fromName",
      "displayName": "From Name",
      "type": "string",
      "default": ""
    },
    {
      "name": "priority",
      "displayName": "Priority",
      "type": "number",
      "default": 2
    },
    {
      "name": "replyTo",
      "displayName": "Reply To",
      "type": "string",
      "default": "",
      "description": "The reply-to email address. Multiple ones can be separated by comma."
    },
    {
      "name": "templateLanguage",
      "displayName": "Template Language",
      "type": "boolean",
      "default": false
    },
    {
      "name": "trackClicks",
      "displayName": "Track Clicks",
      "type": "options",
      "default": null,
      "description": "Use the values specified in the Mailjet account"
    },
    {
      "name": "Disabled",
      "default": null,
      "description": "Disable tracking for this message"
    },
    {
      "name": "Enabled",
      "default": null,
      "description": "Enable tracking for this message"
    },
    {
      "name": "trackOpens",
      "displayName": "Track Opens",
      "type": "options",
      "default": null,
      "description": "Use the values specified in the Mailjet account"
    },
    {
      "name": "variablesJson",
      "displayName": "Variables (JSON)",
      "type": "string",
      "default": "",
      "description": "HTML text message of email"
    },
    {
      "name": "variablesUi",
      "displayName": "Variables",
      "type": "fixedCollection",
      "default": {},
      "placeholder": "Add Variable"
    },
    {
      "name": "value",
      "displayName": "Value",
      "type": "string",
      "default": ""
    },
    {
      "name": "templateId",
      "displayName": "Template Name or ID",
      "type": "options",
      "default": "",
      "description": "Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>",
      "required": true
    },
    {
      "name": "ccEmail",
      "displayName": "Cc Email",
      "type": "string",
      "default": "",
      "description": "Cc recipients of the email separated by ,"
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": null,
      "description": "Send a email"
    },
    {
      "name": "Send Template",
      "default": null,
      "description": "Send a email template"
    },
    {
      "name": "from",
      "displayName": "From",
      "type": "string",
      "default": "",
      "description": "Customizable sender name. Should be between 3 and 11 characters in length, only alphanumeric characters are allowed.",
      "required": true
    },
    {
      "name": "to",
      "displayName": "To",
      "type": "string",
      "default": "",
      "description": "Message recipient. Should be between 3 and 15 characters in length. The number always starts with a plus sign followed by a country code, followed by the number. Phone numbers are expected to comply with the E.164 format.",
      "required": true
    },
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": null
    },
    {
      "name": "SMS",
      "default": null
    }
  ],
  "credentialsConfig": [
    {
      "name": "mailjetEmailApi",
      "required": true
    },
    {
      "name": "resource",
      "required": false
    }
  ]
}