{
  "nodeType": "n8n-nodes-base.gitlabTrigger",
  "displayName": "GitLab Trigger",
  "description": "Starts the workflow when GitLab events occur",
  "version": 1,
  "properties": [
    {
      "name": "authentication",
      "displayName": "Authentication",
      "type": "options",
      "default": "accessToken",
      "options": [
        {
          "name": "Access Token",
          "value": "accessToken"
        },
        {
          "name": "OAuth2",
          "value": "oAuth2"
        }
      ]
    },
    {
      "name": "owner",
      "displayName": "Repository Owner",
      "type": "string",
      "default": "",
      "description": "Owner of the repository",
      "placeholder": "n8n-io",
      "required": true
    },
    {
      "name": "repository",
      "displayName": "Repository Name",
      "type": "string",
      "default": "",
      "description": "The name of the repository",
      "placeholder": "n8n",
      "required": true
    },
    {
      "name": "events",
      "displayName": "Events",
      "type": "multiOptions",
      "default": [],
      "description": "Any time any event is triggered (Wildcard Event)",
      "required": true,
      "options": [
        {
          "name": "*",
          "value": "*",
          "description": "Any time any event is triggered (Wildcard Event)"
        }
      ]
    }
  ],
  "credentialsConfig": [
    {
      "name": "gitlabApi",
      "required": true
    },
    {
      "name": "default",
      "required": false
    },
    {
      "name": "authentication",
      "required": false
    },
    {
      "name": "owner",
      "required": true
    }
  ],
  "io": {
    "inputs": [],
    "outputs": [],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [],
    "produces": []
  }
}