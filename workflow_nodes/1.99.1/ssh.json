{
  "nodeType": "n8n-nodes-base.ssh",
  "displayName": "SSH",
  "description": "Execute commands via SSH",
  "version": 1,
  "properties": [
    {
      "name": "authentication",
      "displayName": "Authentication",
      "type": "options",
      "default": "password",
      "options": [
        {
          "name": "Password",
          "value": "password"
        },
        {
          "name": "Private Key",
          "value": "privateKey"
        }
      ]
    },
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": "command",
      "options": [
        {
          "name": "Command",
          "value": "command"
        },
        {
          "name": "File",
          "value": "file"
        }
      ]
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "execute",
      "description": "Execute a command",
      "options": [
        {
          "name": "Execute",
          "value": "execute",
          "description": "Execute a command"
        }
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "command"
          ]
        }
      }
    },
    {
      "name": "command",
      "displayName": "Command",
      "type": "string",
      "default": "",
      "description": "The command to be executed on a remote device",
      "displayOptions": {
        "show": {
          "resource": [
            "command"
          ],
          "operation": [
            "execute"
          ]
        }
      }
    },
    {
      "name": "cwd",
      "displayName": "Working Directory",
      "type": "string",
      "default": "/",
      "required": true,
      "displayOptions": {
        "show": {
          "resource": [
            "command"
          ],
          "operation": [
            "execute"
          ]
        }
      }
    },
    {
      "name": "binaryPropertyName",
      "displayName": "Input Binary Field",
      "type": "string",
      "default": "data",
      "placeholder": "",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "upload"
          ],
          "resource": [
            "file"
          ]
        }
      }
    },
    {
      "name": "path",
      "displayName": "Target Directory",
      "type": "string",
      "default": "",
      "description": "The directory to upload the file to. The name of the file does not need to be specified, it's taken from the binary data file name. To override this behavior, set the parameter \"File Name\" under options.",
      "placeholder": "/home/user",
      "required": true,
      "displayOptions": {
        "show": {
          "resource": [
            "file"
          ],
          "operation": [
            "upload"
          ]
        }
      }
    },
    {
      "name": "options",
      "displayName": "Options",
      "type": "collection",
      "default": {},
      "description": "Overrides the binary data file name",
      "placeholder": "Add option",
      "options": [
        {
          "name": "fileName",
          "displayName": "File Name",
          "type": "string",
          "default": "",
          "description": "Overrides the binary data file name"
        }
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "file"
          ],
          "operation": [
            "upload",
            "download"
          ]
        }
      }
    }
  ],
  "credentialsConfig": [
    {
      "name": "sshPassword",
      "required": true
    },
    {
      "name": "authentication",
      "required": false
    },
    {
      "name": "resource",
      "required": false
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "command",
      "required": true
    },
    {
      "name": "binaryPropertyName",
      "required": true
    }
  ],
  "io": {
    "inputs": [
      "Main"
    ],
    "outputs": [
      "Main"
    ],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [
      "Main"
    ],
    "produces": [
      "Main"
    ]
  }
}