{
  "nodeType": "n8n-nodes-base.travisCi",
  "displayName": "TravisCI",
  "description": "Consume TravisCI API",
  "version": 1,
  "properties": [
    {
      "name": "buildId",
      "displayName": "Build ID",
      "type": "string",
      "default": "",
      "description": "Value uniquely identifying the build",
      "displayOptions": {
        "show": {
          "operation": [
            "cancel"
          ],
          "resource": [
            "build"
          ]
        }
      }
    },
    {
      "name": "additionalFields",
      "displayName": "Additional Fields",
      "type": "collection",
      "default": {},
      "description": "List of attributes to eager load",
      "placeholder": "Add Field",
      "options": [
        {
          "name": "include",
          "displayName": "Include",
          "type": "string",
          "default": "",
          "description": "List of attributes to eager load",
          "placeholder": "build.commit"
        }
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "build"
          ],
          "operation": [
            "get"
          ]
        }
      }
    },
    {
      "name": "returnAll",
      "displayName": "Return All",
      "type": "boolean",
      "default": false,
      "description": "Whether to return all results or only up to a given limit",
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "resource": [
            "build"
          ]
        }
      }
    },
    {
      "name": "limit",
      "displayName": "Limit",
      "type": "number",
      "default": 100,
      "description": "Max number of results to return",
      "typeOptions": {
        "minValue": 1,
        "maxValue": 500
      },
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "resource": [
            "build"
          ],
          "returnAll": [
            false
          ]
        }
      }
    },
    {
      "name": "slug",
      "displayName": "Slug",
      "type": "string",
      "default": "",
      "description": "Same as {ownerName}/{repositoryName}",
      "placeholder": "n8n-io/n8n",
      "displayOptions": {
        "show": {
          "operation": [
            "trigger"
          ],
          "resource": [
            "build"
          ]
        }
      }
    },
    {
      "name": "branch",
      "displayName": "Branch",
      "type": "string",
      "default": "",
      "description": "Branch requested to be built",
      "placeholder": "master",
      "displayOptions": {
        "show": {
          "operation": [
            "trigger"
          ],
          "resource": [
            "build"
          ]
        }
      }
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "cancel",
      "description": "Cancel a build",
      "options": [
        {
          "name": "Cancel",
          "value": "cancel",
          "description": "Cancel a build"
        },
        {
          "name": "Get",
          "value": "get",
          "description": "Get a build"
        },
        {
          "name": "Get Many",
          "value": "getAll",
          "description": "Get many builds"
        },
        {
          "name": "Restart",
          "value": "restart",
          "description": "Restart a build"
        },
        {
          "name": "Trigger",
          "value": "trigger",
          "description": "Trigger a build"
        }
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "build"
          ]
        }
      }
    },
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": "build",
      "options": [
        {
          "name": "Build",
          "value": "build"
        }
      ]
    }
  ],
  "credentialsConfig": [
    {
      "name": "travisCiApi",
      "required": true
    },
    {
      "name": "resource",
      "required": false
    }
  ],
  "io": {
    "inputs": [
      "Main"
    ],
    "outputs": [
      "Main"
    ],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [
      "Main"
    ],
    "produces": [
      "Main"
    ]
  }
}