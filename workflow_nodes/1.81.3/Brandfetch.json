{
  "nodeType": "n8n-nodes-base.Brandfetch",
  "displayName": "Brandfetch",
  "description": "Consume Brandfetch API",
  "version": 1,
  "properties": [
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "logo",
      "description": "Return a company's colors",
      "options": [
        {
          "name": "Color",
          "value": "color",
          "description": "Return a company's colors"
        },
        {
          "name": "Company",
          "value": "company",
          "description": "Return a company's data"
        },
        {
          "name": "Font",
          "value": "font",
          "description": "Return a company's fonts"
        },
        {
          "name": "Industry",
          "value": "industry",
          "description": "Return a company's industry"
        },
        {
          "name": "Logo",
          "value": "logo",
          "description": "Return a company's logo & icon"
        }
      ]
    },
    {
      "name": "domain",
      "displayName": "Domain",
      "type": "string",
      "default": "",
      "description": "The domain name of the company",
      "required": true
    },
    {
      "name": "download",
      "displayName": "Download",
      "type": "boolean",
      "default": false,
      "description": "Name of the binary property to which to write the data of the read file",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "logo"
          ]
        }
      }
    },
    {
      "name": "imageTypes",
      "displayName": "Image Type",
      "type": "multiOptions",
      "default": "['logo', 'icon']",
      "required": true,
      "options": [
        {
          "name": "Icon",
          "value": "icon"
        },
        {
          "name": "Logo",
          "value": "logo"
        }
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "logo"
          ],
          "download": [
            true
          ]
        }
      }
    },
    {
      "name": "imageFormats",
      "displayName": "Image Format",
      "type": "multiOptions",
      "default": "['png']",
      "description": "The image format in which the logo should be returned as",
      "required": true,
      "options": [
        {
          "name": "PNG",
          "value": "png"
        },
        {
          "name": "SVG",
          "value": "svg"
        }
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "logo"
          ],
          "download": [
            true
          ]
        }
      }
    }
  ],
  "credentialsConfig": [
    {
      "name": "brandfetchApi",
      "required": true
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "domain",
      "required": true
    },
    {
      "name": "imageFormats",
      "required": false
    }
  ],
  "io": {
    "inputs": [],
    "outputs": [],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [],
    "produces": []
  }
}