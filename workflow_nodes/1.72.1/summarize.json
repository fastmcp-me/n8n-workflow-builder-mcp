    {
      "name": "Average",
      "default": null
    },
    {
      "name": "Concatenate",
      "default": null
    },
    {
      "name": "Count",
      "default": null
    },
    {
      "name": "Count Unique",
      "default": null
    },
    {
      "name": "Max",
      "default": null
    },
    {
      "name": "Min",
      "default": null
    },
    {
      "name": "Sum",
      "default": null
    },
    {
      "name": "field",
      "displayName": "Field",
      "type": "string",
      "default": "",
