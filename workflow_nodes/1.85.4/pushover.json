{
  "nodeType": "n8n-nodes-base.pushover",
  "displayName": "Pushover",
  "description": "Consume Pushover API",
  "version": 1,
  "properties": [
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": "message",
      "options": [
        {
          "name": "Message",
          "value": "message"
        }
      ]
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "push",
      "options": [
        {
          "name": "Push",
          "value": "push"
        }
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "message"
          ]
        }
      }
    },
    {
      "name": "userKey",
      "displayName": "User Key",
      "type": "string",
      "default": "",
      "description": "The user/group key (not e-mail address) of your user (or you), viewable when logged into the <a href=\"https://pushover.net/\">dashboard</a> (often referred to as <code>USER_KEY</code> in the <a href=\"https://support.pushover.net/i44-example-code-and-pushover-libraries\">libraries</a> and code examples)",
      "required": true,
      "displayOptions": {
        "show": {
          "resource": [
            "message"
          ],
          "operation": [
            "push"
          ]
        }
      }
    },
    {
      "name": "message",
      "displayName": "Message",
      "type": "string",
      "default": "",
      "description": "Your message",
      "required": true,
      "displayOptions": {
        "show": {
          "resource": [
            "message"
          ],
          "operation": [
            "push"
          ]
        }
      }
    },
    {
      "name": "priority",
      "displayName": "Priority",
      "type": "options",
      "default": null,
      "description": "Send as -2 to generate no notification/alert, -1 to always send as a quiet notification, 1 to display as high-priority and bypass the user's quiet hours, or 2 to also require confirmation from the user",
      "options": [
        {
          "name": "Lowest Priority"
        },
        {
          "name": "Low Priority"
        },
        {
          "name": "Normal Priority",
          "value": 0
        },
        {
          "name": "High Priority",
          "value": 1
        },
        {
          "name": "Emergency Priority",
          "value": 2
        }
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "message"
          ],
          "operation": [
            "push"
          ]
        }
      }
    },
    {
      "name": "retry",
      "displayName": "Retry (Seconds)",
      "type": "number",
      "default": 30,
      "description": "Specifies how often (in seconds) the Pushover servers will send the same notification to the user. This parameter must have a value of at least 30 seconds between retries.",
      "required": true,
      "typeOptions": {
        "minValue": 0
      },
      "displayOptions": {
        "show": {
          "resource": [
            "message"
          ],
          "operation": [
            "push"
          ],
          "priority": [
            2
          ]
        }
      }
    },
    {
      "name": "expire",
      "displayName": "Expire (Seconds)",
      "type": "number",
      "default": 30,
      "description": "Specifies how many seconds your notification will continue to be retried for (every retry seconds)",
      "required": true,
      "typeOptions": {
        "minValue": 0,
        "maxValue": 10800
      },
      "displayOptions": {
        "show": {
          "resource": [
            "message"
          ],
          "operation": [
            "push"
          ],
          "priority": [
            2
          ]
        }
      }
    },
    {
      "name": "additionalFields",
      "displayName": "Additional Fields",
      "type": "collection",
      "default": {},
      "description": "Your user's device name to send the message directly to that device, rather than all of the user's devices (multiple devices may be separated by a comma)",
      "placeholder": "Add Field",
      "displayOptions": {
        "show": {
          "resource": [
            "message"
          ],
          "operation": [
            "push"
          ]
        }
      }
    }
  ],
  "credentialsConfig": [
    {
      "name": "pushoverApi",
      "required": true
    },
    {
      "name": "resource",
      "required": false
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "userKey",
      "required": true
    },
    {
      "name": "retry",
      "required": true
    },
    {
      "name": "device",
      "required": false
    }
  ],
  "io": {
    "inputs": [
      "Main"
    ],
    "outputs": [
      "Main"
    ],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
