{
  "nodeType": "n8n-nodes-base.mindee",
  "displayName": "Mindee",
  "description": "Consume Mindee API",
  "version": [
    1,
    2,
    3
  ],
  "properties": [
    {
      "name": "apiVersion",
      "displayName": "API Version",
      "type": "options",
      "default": 1,
      "description": "Which Mindee API Version to use",
      "options": [
        {
          "name": "1",
          "value": 1
        },
        {
          "name": "3",
          "value": 3
        },
        {
          "name": "4",
          "value": 4
        }
      ],
      "displayOptions": {
        "show": {
          "@version": [
            1
          ]
        }
      }
    },
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": "receipt",
      "options": [
        {
          "name": "Invoice",
          "value": "invoice"
        },
        {
          "name": "Receipt",
          "value": "receipt"
        }
      ]
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "predict",
      "options": [
        {
          "name": "Predict",
          "value": "predict"
        }
      ]
    },
    {
      "name": "binaryPropertyName",
      "displayName": "Input Binary Field",
      "type": "string",
      "default": "data",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "predict"
          ],
          "resource": [
            "receipt",
            "invoice"
          ]
        }
      }
    },
    {
      "name": "rawData",
      "displayName": "RAW Data",
      "type": "boolean",
      "default": false,
      "description": "Whether to return the data exactly in the way it got received from the API"
    }
  ],
  "credentialsConfig": [
    {
      "name": "mindeeReceiptApi",
      "required": true
    },
    {
      "name": "apiVersion",
      "required": false
    },
    {
      "name": "apiVersion",
      "required": false
    },
    {
      "name": "apiVersion",
      "required": false
    },
    {
      "name": "resource",
      "required": false
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "binaryPropertyName",
      "required": true
    }
  ],
  "io": {
    "inputs": [],
    "outputs": [],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [],
    "produces": []
  }
}