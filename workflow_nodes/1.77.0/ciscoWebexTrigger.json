{
  "nodeType": "n8n-nodes-base.ciscoWebexTrigger",
  "displayName": "Webex by Cisco Trigger",
  "description": "Starts the workflow when Cisco Webex events occur.",
  "version": 1,
  "properties": [
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": "meeting",
      "required": true
    },
    {
      "name": "resolveData",
      "displayName": "Resolve Data",
      "type": "boolean",
      "default": true,
      "description": "By default the response only contain a reference to the data the user inputed. If this option gets activated, it will resolve the data automatically.",
      "displayOptions": {
        "show": {
          "resource": [
            "attachmentAction"
          ]
        }
      }
    },
    {
      "name": "filters",
      "displayName": "Filters",
      "type": "collection",
      "default": {},
      "description": "Whether to limit to messages which contain file content attachments",
      "placeholder": "Add Filter",
      "displayOptions": {
        "show": {
          "/resource": [
            "message"
          ],
          "/event": [
            "created",
            "deleted"
          ]
        }
      }
    }
  ],
  "credentialsConfig": [
    {
      "name": "ciscoWebexOAuth2Api",
      "required": true
    },
    {
      "name": "default",
      "required": false
    },
    {
      "name": "resource",
      "required": false
    },
    {
      "name": "resolveData",
      "required": false
    },
    {
      "name": "type",
      "required": false
    },
    {
      "name": "callType",
      "required": false
    }
  ],
  "io": {
    "inputs": [],
    "outputs": [],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [],
    "produces": []
  }
}