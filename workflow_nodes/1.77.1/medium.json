{
  "nodeType": "n8n-nodes-base.medium",
  "displayName": "Medium",
  "description": "Consume Medium API",
  "version": 1,
  "properties": [
    {
      "name": "authentication",
      "displayName": "Authentication",
      "type": "options",
      "default": "accessToken",
      "options": [
        {
          "name": "Access Token",
          "value": "accessToken"
        },
        {
          "name": "OAuth2",
          "value": "oAuth2"
        }
      ]
    },
    {
      "name": "resource",
      "displayName": "Resource",
      "type": "options",
      "default": "post",
      "options": [
        {
          "name": "Post",
          "value": "post"
        },
        {
          "name": "Publication",
          "value": "publication"
        }
      ]
    },
    {
      "name": "operation",
      "displayName": "Operation",
      "type": "options",
      "default": "create",
      "description": "Create a post",
      "options": [
        {
          "name": "Create",
          "value": "create",
          "description": "Create a post"
        }
      ],
      "displayOptions": {
        "show": {
          "resource": [
            "post"
          ]
        }
      }
    },
    {
      "name": "publication",
      "displayName": "Publication",
      "type": "boolean",
      "default": false,
      "description": "Whether you are posting for a publication",
      "displayOptions": {
        "show": {
          "resource": [
            "post"
          ],
          "operation": [
            "create"
          ]
        }
      }
    },
    {
      "name": "publicationId",
      "displayName": "Publication Name or ID",
      "type": "options",
      "default": "",
      "description": "Publication IDs. Choose from the list, or specify an ID using an <a href=\"https://docs.n8n.io/code/expressions/\">expression</a>.",
      "displayOptions": {
        "show": {
          "resource": [
            "post"
          ],
          "operation": [
            "create"
          ],
          "publication": [
            true
          ]
        }
      }
    },
    {
      "name": "title",
      "displayName": "Title",
      "type": "string",
      "default": "",
      "description": "Title of the post. Max Length : 100 characters.",
      "placeholder": "My Open Source Contribution",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "create"
          ],
          "resource": [
            "post"
          ]
        }
      }
    },
    {
      "name": "contentFormat",
      "displayName": "Content Format",
      "type": "options",
      "default": "",
      "description": "The format of the content to be posted",
      "required": true,
      "options": [
        {
          "name": "HTML",
          "value": "html"
        },
        {
          "name": "Markdown",
          "value": "markdown"
        }
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "create"
          ],
          "resource": [
            "post"
          ]
        }
      }
    },
    {
      "name": "content",
      "displayName": "Content",
      "type": "string",
      "default": "",
      "description": "The body of the post, in a valid semantic HTML fragment, or Markdown",
      "placeholder": "My open source contribution",
      "required": true,
      "displayOptions": {
        "show": {
          "operation": [
            "create"
          ],
          "resource": [
            "post"
          ]
        }
      }
    },
    {
      "name": "additionalFields",
      "displayName": "Additional Fields",
      "type": "collection",
      "default": {},
      "description": "The original home of this content, if it was originally published elsewhere",
      "placeholder": "Add Field",
      "options": [
        {
          "name": "canonicalUrl",
          "displayName": "Canonical Url",
          "type": "string",
          "default": "",
          "description": "The original home of this content, if it was originally published elsewhere"
        }
      ],
      "displayOptions": {
        "show": {
          "operation": [
            "create"
          ],
          "resource": [
            "post"
          ]
        }
      }
    },
    {
      "name": "returnAll",
      "displayName": "Return All",
      "type": "boolean",
      "default": false,
      "description": "Whether to return all results or only up to a given limit",
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "resource": [
            "publication"
          ]
        }
      }
    },
    {
      "name": "limit",
      "displayName": "Limit",
      "type": "number",
      "default": 100,
      "description": "Max number of results to return",
      "typeOptions": {
        "minValue": 1,
        "maxValue": 200
      },
      "displayOptions": {
        "show": {
          "operation": [
            "getAll"
          ],
          "resource": [
            "publication"
          ],
          "returnAll": [
            false
          ]
        }
      }
    }
  ],
  "credentialsConfig": [
    {
      "name": "mediumApi",
      "required": true
    },
    {
      "name": "authentication",
      "required": false
    },
    {
      "name": "resource",
      "required": false
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "publication",
      "required": true
    },
    {
      "name": "content",
      "required": true
    },
    {
      "name": "notifyFollowers",
      "required": false
    },
    {
      "name": "tags",
      "required": false
    },
    {
      "name": "operation",
      "required": false
    },
    {
      "name": "returnAll",
      "required": false
    }
  ],
  "io": {
    "inputs": [],
    "outputs": [],
    "outputNames": [],
    "hints": {}
  },
  "wiring": {
    "role": "generic",
    "requires": [],
    "optional": [],
    "consumedBy": [],
    "consumes": [],
    "produces": []
  }
}